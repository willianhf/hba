{
  "name": "@hba/api",
  "version": "1.0.0",
  "scripts": {
    "dev": "tsx watch src/index.ts --clear-screen=false",
    "type-check": "tsc --pretty --noEmit",
    "start": "node --experimental-specifier-resolution=node dist/index.js",
    "build:production": "npm run migrate:deploy && npm run database:seed && npm run build",
    "build": "swc src/ -d dist/ -s",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "database:seed": "prisma db seed"
  },
  "dependencies": {
    "@discordx/utilities": "^5.2.1",
    "@pothos/core": "^3.24.0",
    "@pothos/plugin-errors": "^3.1.1",
    "@pothos/plugin-relay": "^3.6.0",
    "@pothos/plugin-scope-auth": "^3.4.2",
    "@pothos/plugin-simple-objects": "^3.0.3",
    "@prisma/client": "^3.4.1",
    "@swan-io/boxed": "^0.10.0",
    "@trpc/server": "^9.26.2",
    "axios": "^1.2.6",
    "bcrypt": "^5.0.1",
    "common-tags": "^1.8.2",
    "discord.js": "^14.7.1",
    "discordx": "^11.5.2",
    "dotenv": "^10.0.0",
    "express": "^4.18.1",
    "graphql": "16.6.0",
    "graphql-yoga": "^3.3.0",
    "jsonwebtoken": "^8.5.1",
    "uuid": "^8.3.2",
    "yup": "^0.32.11",
    "zod": "^3.17.10"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.30",
    "@swc/helpers": "^0.4.14",
    "@types/bcrypt": "^5.0.0",
    "@types/common-tags": "^1.8.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^16.11.6",
    "@types/uuid": "^8.3.4",
    "prisma": "^3.4.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "tsx": "^3.8.2",
    "typescript": "^4.6.2"
  },
  "prisma": {
    "schema": "src/shared/infra/database/Prisma/schema.prisma",
    "seed": "tsx src/shared/infra/database/Prisma/seed/index.ts"
  }
}
